%Equal hat
\newcommand\equalhat{%
\let\savearraystretch\arraystretch
\renewcommand\arraystretch{0.3}
\begin{array}{c}
\stretchto{
    \scalerel*[\widthof{=}]{\wedge}
    {\rule{1ex}{3ex}}%
}{0.5ex}\\ 
=%
\end{array}
\let\arraystretch\savearraystretch
}

% Load figures command: \inputfig [htb]{fig_dir}{fig_label}
\newcommand*{\inputfig}[3][htb]{{
    \def\fps@figure{#1}
    \def\DIR{#2}
    \def\LABEL{#3}
    \graphicspath{{\DIR/}}
    \input{\DIR/fig.tex}
}}

% Load table command: \inputtab [htb]{tab_file}{table_label}
\newcommand*{\inputtab}[3][htb]{{
    \def\fps@table{#1}
    \def\nametable{#2}
    \def\LABEL{#3}
    \input{\nametable}
}}

